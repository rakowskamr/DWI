<?xml version="1.0"?>
<pipeline>
	<version>advanced</version>
	<reporting>on</reporting>
	<image>
		<label>DWimage1</label>
		<class>DWimage</class>
		<preproc>
			<method>Supp_human_bet_mask</method>
			<handle></handle>
		</preproc>
		<preproc>
			<method>DWI_proc_SOLID</method>
			<lower_threshold>3.5</lower_threshold>
			<upper_threshold>10</upper_threshold>
			<olmask>yes</olmask>
			<modZ>yes</modZ>
			<metric>1</metric>
			<handle></handle>
		</preproc>
	</image>
	<image>
		<label>DWpaRef2</label>
		<class>DWpaRef</class>
		<preproc></preproc>
	</image>
	<processing>
		<process>
			<method>DWI_proc_gibbs_SubVoxShift</method>
			<handle></handle>
		</process>
		<process>
			<method>DWI_sort_refPA</method>
			<ref>common</ref>
			<handle></handle>
		</process>
		<process>
			<method>DWI_proc_TED</method>
			<maskf>0.4</maskf>
			<scanner>connectom</scanner>
			<lower_threshold>3.5</lower_threshold>
			<upper_threshold>10</upper_threshold>
			<xhack>no</xhack>
			<handle></handle>
		</process>
		<process>
			<method>DWI_proc_DTI_fit</method>
			<fit>IRLLS</fit>
			<maskf>0.4</maskf>
			<itts>100</itts>
			<tol>1e-5</tol>
			<minb>400</minb>
			<shell>1200</shell>
			<lowb>500</lowb>
			<nitts>20</nitts>
			<handle></handle>
		</process>
		<process>
			<method>DWI_proc_DTI_maps</method>
			<handle></handle>
		</process>
		<process>
			<method>DWI_proc_CHARMED_fit</method>
			<model>standard</model>
			<nres>3</nres>
			<lambda>1</lambda>
			<handle></handle>
		</process>
		<process>
			<method>DWI_proc_CHARMED_maps</method>
			<handle></handle>
		</process>
	</processing>
</pipeline>
